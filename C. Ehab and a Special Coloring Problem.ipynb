    "n = int(input())\n",
    "c = 0\n",
    "ans = [0 for x in range(10)]\n",
    "for i in range(2,n+1):\n",
    "    if not ans[i]:\n",
    "        c+=1\n",
    "        ans[i]=c\n",
    "        for j in range(i, n+1, i):\n",
    "            ans[j] = ans[i]\n",
    "    print(ans[i], end=\" \")"
   ]
