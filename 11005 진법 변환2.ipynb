   "source": [
    "import string\n",
    "N, B = map(int, input().split())\n",
    "d = list('0123456789')\n",
    "d+=list(string.ascii_uppercase)\n",
    "res =''\n",
    "while N>=B:\n",
    "    s = N%B\n",
    "    res+=d[s]\n",
    "    N=N//B\n",
    "res+=d[N]\n",
    "print(res[::-1])"
   ]
