    "n, m = map(int, input().split())\n",
    "def gcd(m, n):\n",
    "    if m%n == 0:\n",
    "        return n\n",
    "    else:\n",
    "        return gcd(n, m%n)\n",
    "print(gcd(n, m))\n",
    "print(m*n//gcd(n,m))"
   ]
